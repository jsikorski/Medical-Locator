@using MedicalLocator.WebFront.Models
@model IDictionary<MedicalType, bool>
    
@for (int i = 0; i < Model.Count; i++)
{
    <div>
        @Html.CheckBox(string.Format("MedicalTypesDictionary[{0}].Value", i), Model.ElementAt(i).Value)
        @Html.Label(string.Format("MedicalTypesDictionary[{0}].Key", i), Model.ElementAt(i).Key.ToString())
        @Html.Hidden(string.Format("MedicalTypesDictionary[{0}].Key", i), Model.ElementAt(i).Key.ToString())
    </div>
}
