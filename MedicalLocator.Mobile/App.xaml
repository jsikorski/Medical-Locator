<Application x:Class="MedicalLocator.Mobile.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Mobile="clr-namespace:MedicalLocator.Mobile">
    <Application.Resources>
        <ResourceDictionary>
            <Mobile:AppBootstrapper x:Key="bootstrapper" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/BusyIndicatorControlStyle.xaml" />
                <ResourceDictionary Source="Styles/PushpinsStyle.xaml" />
                <ResourceDictionary Source="Styles/PushpinsTooltipStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>