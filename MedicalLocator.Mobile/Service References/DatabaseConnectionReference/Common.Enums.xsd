      <xs:element minOccurs="0" maxOccurs="unbounded" name="MedicalType" type="tns:MedicalType" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfMedicalType" nillable="true" type="tns:ArrayOfMedicalType" />
